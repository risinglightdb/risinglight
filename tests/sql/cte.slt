# create a CTE called "cte" and use it in the main query
query I
WITH cte AS (SELECT 42 AS x)
SELECT * FROM cte;
----
42

# create two CTEs, where the second CTE references the first CTE
query I
WITH cte AS (SELECT 42 AS i),
     cte2 AS (SELECT i*100 AS x FROM cte)
SELECT * FROM cte2;
----
4200


